    "nodemon": "^1.19.4",
    "passport": "^0.3.2",
    "passport-google-oauth20": "^1.0.0",
    "path-parser": "^2.0.2",
    "puppeteer": "^1.20.0",
    "redis": "^3.1.1",
    "uuid": "^3.3.3"
  },
  "standard": {
